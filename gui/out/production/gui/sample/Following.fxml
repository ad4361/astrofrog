<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="431.0" prefWidth="680.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FollowingController">
    <children>
        <Label layoutX="24.0" layoutY="28.0" prefHeight="38.0" prefWidth="193.0" text="Who you follow">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <Button fx:id="goBackButton" layoutX="14.0" layoutY="376.0" mnemonicParsing="false" onAction="#switchToMainPageScene" prefHeight="37.0" prefWidth="69.0" text="Go back" />
        <Label layoutX="263.0" layoutY="32.0" prefHeight="38.0" prefWidth="151.0" text="Search for user:">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
        <TextField fx:id="searchField" layoutX="385.0" layoutY="37.0" prefHeight="28.0" prefWidth="193.0" promptText="Enter email..." />
        <TableView fx:id="followingTable" layoutX="76.0" layoutY="101.0" prefHeight="257.0" prefWidth="511.0">
            <columns>
                <TableColumn prefWidth="75.0" text="Username" />
                <TableColumn prefWidth="75.0" text="Unfollow" />
            </columns>
        </TableView>
        <Button fx:id="searchButton" layoutX="596.0" layoutY="28.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="69.0" text="Search" />
    </children>
</AnchorPane>
