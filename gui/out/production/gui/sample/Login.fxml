<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="341.0" prefWidth="546.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Label layoutX="218.0" layoutY="14.0" text="Login">
         <font>
            <Font size="45.0" />
         </font>
      </Label>
      <Button fx:id="goBackButton" layoutX="39.0" layoutY="301.0" mnemonicParsing="false" onAction="#switchToWelcomeScene" text="Go back" />
      <PasswordField fx:id="passwordField" layoutX="198.0" layoutY="171.0" promptText="password" />
      <TextField fx:id="usernameField" layoutX="198.0" layoutY="124.0" promptText="username" />
      <Button fx:id="loginButton" layoutX="250.0" layoutY="213.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Label layoutX="198.0" layoutY="107.0" text="Username" />
      <Label layoutX="198.0" layoutY="154.0" text="Password" />
      <Label fx:id="loginMessageLabel" layoutX="166.0" layoutY="255.0" prefHeight="25.0" prefWidth="213.0" textFill="#e20909" />
   </children>
</AnchorPane>
