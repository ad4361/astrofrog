<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="341.0" prefWidth="546.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignupController">
   <children>
      <Label layoutX="203.0" layoutY="14.0" text="Signup">
         <font>
            <Font size="45.0" />
         </font>
      </Label>
      <Button fx:id="goBackButton" layoutX="24.0" layoutY="302.0" mnemonicParsing="false" onAction="#switchToWelcomeScene" text="Go back" />
      <TextField fx:id="usernameField" layoutX="54.0" layoutY="118.0" promptText="username" />
      <TextField fx:id="passwordField" layoutX="54.0" layoutY="170.0" promptText="password" />
      <TextField fx:id="firstNameField" layoutX="319.0" layoutY="118.0" promptText="first name" />
      <TextField fx:id="lastNameField" layoutX="319.0" layoutY="170.0" promptText="last name" />
      <TextField fx:id="emailField" layoutX="54.0" layoutY="220.0" promptText="email" />
      <Label layoutX="55.0" layoutY="101.0" text="Username" />
      <Label layoutX="55.0" layoutY="150.0" text="Password" />
      <Label layoutX="55.0" layoutY="203.0" text="Email" />
      <Label layoutX="319.0" layoutY="101.0" text="First name" />
      <Label layoutX="319.0" layoutY="150.0" text="Last name" />
      <DatePicker fx:id="dobField" layoutX="319.0" layoutY="220.0" prefHeight="25.0" prefWidth="149.0" promptText="MM/DD/YYYY" />
      <Label layoutX="319.0" layoutY="203.0" text="Date of birth" />
      <Button fx:id="signupButton" layoutX="235.0" layoutY="264.0" mnemonicParsing="false" onAction="#signup" text="Signup" />
   </children>
</AnchorPane>
