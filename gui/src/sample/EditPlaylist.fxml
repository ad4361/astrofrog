      </Label>
      <Label layoutX="106.0" layoutY="106.0" text="Rename playlist to:" />
      <Button layoutX="12.0" layoutY="98.0" mnemonicParsing="false" onAction="#switchToPLDetailsScene" prefHeight="33.0" prefWidth="59.0" text="Go back" />
      <Label layoutX="12.0" layoutY="51.0" prefHeight="17.0" prefWidth="187.0" text="Current playlist name:">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label fx:id="plNameLabel" layoutX="141.0" layoutY="51.0" prefHeight="17.0" prefWidth="365.0">
